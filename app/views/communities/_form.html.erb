<%= form_with model: @community, local: true do |f| %>
  <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  
  <div>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  
  <div>
    <%= f.label :profile_picture %>
    <%= f.file_field :profile_picture %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
